{
"manifest_version": 2,
    "name": "Diaeresizer",
    "description": "Adds diaereses to common words.",
    "version": "1.0",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "replacement.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "words.json"
    ]
}